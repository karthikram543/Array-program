public class name {

	public static void main(String[] args) {
		
		int arr[]= {1,20,300,20,2100};
		int[] arr1=new int[arr.length];
		int j=arr.length-1;
	//
		
		for(int i=0;i<arr.length;i++) {
			
			arr1[i]=arr[j];
			j--;
			
			
			System.out.println(arr1[i]);
			
		}
    }
    }
		
	output:
2100
20
300
20
1
			
		
		
		
		
		
		
	
