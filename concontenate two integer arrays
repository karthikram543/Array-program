public class This {
		public static void main(String[] args) {
		
			int j=0;
			
			int arr[]= {1,2,3,4,5};
			int brr[]= {6,7,8,9};
			int result[]=new int[arr.length+brr.length];
			
		
			for( int i=0;i<result.length;i++) {
				
				if(i<arr.length) {
					result[i]=arr[i];
					System.out.println(result[i]);
					
				}
				else  {
					result[i]=brr[j];
					j++;
					System.out.println(result[i]);
				}
				
				
			
				
				
				
				
			}
			
		
	
			
		}
}
		
		

