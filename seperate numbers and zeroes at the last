public class concat {
	
	
	
	public static void main (String[]args) {
		
		int array[]= {3,0,4,0,5,8,0,0};
		
		
		
		
		for(int i=0;i<array.length;i++) {
			
			for(int j=i+1;j<array.length;j++) {
				int temp=0;
				if(array[i]<array[j]) {
					temp=array[i];
					array[i]=array[j];
					array[j]=temp;
					
				}
				
			}
			System.out.println(array[i]);
			
		}
		
		
		
		
		
		

	
}
}
