public class array6 {

	public static void main(String[] args) {
		array6 obj = new array6();
		
		boolean result=obj.min();
		System.out.println(result);
	
	

}
	boolean min() {
		int array[]= {1,2,3-1,4,5};
		for(int i=0;i<array.length;i++) {
			if(array[i]== -1 || array[i]==0) {
				return false;
			}
			
			
		}
		return false;
		
		
	}
		
		
}
