public class array6 {

	public static void main(String[] args) {
		int ar[]= {1,2-1,-1,0,0-1,5,0};
		int temp=0;
		for(int i=0;i<ar.length;i++) {
			
			for(int j=i+1;j<ar.length;j++) {
				if(ar[i]>ar[j]) {
				temp=ar[i];
				ar[i]=ar[j];
				ar[j]=temp;
				
			}
			
		}
			if(ar[i]<=0) {
				System.out.println(ar[i]);
			}

	}
	
	

}
}


output:
-1
-1
0
0
