public class array5 {

	public static void main(String[] args) {
		
		int arr[]= {1,2,3,4,5};
		int big=arr[0];
		int sum=0;
		int small=arr[0];
		int i=0;
		
		for( i=1;i<arr.length;i++) {
			sum=sum+arr[i];
			if(arr[i]>big) {
				big=arr[i];
			}
			else if(arr[i]<small) {
				small=arr[i];
			}
			
			
		}
		float x= ((sum-big-small/(arr.length-2)));
		System.out.println(x);
		

	}

}


output:
9.0
