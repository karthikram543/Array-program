public class Main
{
	public static void main(String[] args) {
	
	  
	      int arr[]={1,2,90,5,6};
	      
	      int big=arr[0];
	      int smallbig=0;
	     
	     for(int i=0;i<arr.length;i++){
	         
	         if(arr[i]>big){
	         smallbig=big;
	         big=arr[i];
	         
	     }
	     else if(arr[i]>smallbig){
	         smallbig=arr[i];
	     }
 
	}
	System.out.println(smallbig);
		System.out.println(big);

	   
	}
}
