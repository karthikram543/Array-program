public class array {

	public static void main(String[] args) {
		int sum=0;
		
		int[] arr = new int[5];
		arr[0]=7;
		arr[1]=5;
		arr[2]=10;
		arr[3]=8;
		arr[4]=7;
		

		for(int i=0;i<arr.length;i++) {
			
			sum=sum+arr[i];
			System.out.println(sum);
		}
		
		
		
	}
}


output :
 7
12
22
30
37
